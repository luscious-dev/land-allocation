                                                        |  356 Sq-Ft
                                                    li
                                                        i.flaticon-bed
                                                        |  4 Bedrooms
                                                    li
                                                        i.flaticon-car
                                                        |  2 Garage
                                                    li
                                                        i.flaticon-bathtub
                                                        |  3 Bathroom
                                                .price $ 13,65,000
                                            .option-box
                                                .expire-date 4.01.2018
