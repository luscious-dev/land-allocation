extends base

block content
    .dashboard
        .container-fluid
            .content-area
                .dashboard-content
                    .dashboard-header.clearfix
                        .row
                            .col-md-6.col-sm-12
                                h4 My Properties
                            .col-md-6.col-sm-12
                                .breadcrumb-nav
                                    ul
                                        li
                                            a(href='../index-2.html') Index
                                        li
                                            a(href='dashboard.html') Dashboard
                                        li.active Properties
                    .row
                        .column.col-lg-12
                            .properties-box
                                .title
                                    h3 My Properties List
                                .inner-container
                                    // Property Block
                                    .property-block
                                        .inner-box.clearfix
                                            .image-box
                                                figure.image
                                                    img(src='/img/resource/property-1.jpg' alt='')
                                            .content-box
                                                h3 Single House Near Orland Park.
                                                .location
                                                    i.la.la-map-marker
                                                    |  Orland Park, IL 35785, Chicago, United State
